{
  "name": "see-fetch",
  "version": "0.2.0",
  "description": "A window.fetch wrapper, with customizing request keys, refactoring response, pre handling, post handling, etc.",
  "homepage": "https://github.com/senntyou/see-fetch",
  "author": "senntyou",
  "keywords": [
    "window.fetch",
    "fetch",
    "wrapper",
    "json",
    "refactor",
    "response",
    "handle"
  ],
  "source": "src/index.js",
  "module": "dist/see-fetch.m.js",
  "umd:main": "dist/see-fetch.umd.js",
  "main": "dist/see-fetch.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/senntyou/see-fetch"
  },
  "scripts": {
    "build": "microbundle --name seeFetch",
    "dev": "microbundle watch",
    "precommit": "npm run lint-staged",
    "prettier": "prettier --write ./**/*.{js,json,md}",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "cover": "npm run cover:init && npm run cover:all && npm run cover:report",
    "cover:init": "rimraf coverage",
    "cover:all": "jest --coverage",
    "cover:report": "istanbul report"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "clone": "^2.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jest": "^23.4.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "lint-staged": "^7.2.0",
    "microbundle": "^0.6.0",
    "node-fetch": "^2.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "url-search-params": "^1.0.2"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "json-refactor": "^0.2.0"
  }
}
